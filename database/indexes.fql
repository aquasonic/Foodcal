CreateIndex({
  name: "uniqueMeal",
  unique: true,
  serialized: true,
  source: Collection("Meal"),
  terms: [
    {
      field: ["data", "date"]
    },
    {
      field: ["data", "type"]
    }
  ]
})

CreateIndex({
  name: "usersByEmail",
  source: Collection("Users"),
  terms: [{field: ["data", "email"]}],
  unique: true,
})